#!groovy
import groovy.json.JsonSlurper

@Library('Infrastructure') _

properties([
  parameters([
    choice(name: 'ENVIRONMENT', choices:  'sbox\ndev\ntest\nstg\nprod', description: 'Environment where code should be build and deployed')
    choice(name: 'SUBSCRIPTION', choices: 'sbox\ndev\ntest\nstg\nprod', description: 'Subscription where code should be deployed to')
  ])
])

def product = 'pre'

withParameterizedInfraPipeline(product, params.ENVIRONMENT, params.SUBSCRIPTION) {
}
